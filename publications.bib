@inproceedings{liu_costco_2019,
	address = {New York, NY, USA},
	series = {{KDD} '19},
	title = {{CoSTCo}: {A} {Neural} {Tensor} {Completion} {Model} for {Sparse} {Tensors}},
	isbn = {978-1-4503-6201-6},
