	month = dec,
	year = {2023},
	note = {arXiv:2303.02304 [cs]},
	keywords = {Computer Science - Machine Learning},
	annote = {Comment: Accepted to ICLR 2023},
	file = {Preprint PDF:C\:\\Users\\selvam\\Zotero\\storage\\7XEAJTNF\\Xiao et al. - 2023 - Coupled Multiwavelet Neural Operator Learning for Coupled Partial Differential Equations.pdf:application/pdf;Snapshot:C\:\\Users\\selvam\\Zotero\\storage\\PXSV2YUN\\2303.html:text/html},
